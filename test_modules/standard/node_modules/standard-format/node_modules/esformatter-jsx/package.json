{
  "name": "esformatter-jsx",
  "version": "1.3.0",
  "description": "esformatter plugin: format javascript files that contain React JSX blocks",
  "repository": {
    "type": "git",
    "url": "https://github.com/royriojas/esformatter-jsx"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Riojas",
    "url": "http://royriojas.com"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "prepush": [
    "npm run check"
  ],
  "scripts": {
    "beautify": "esbeautifier index.js 'specs/**/*.spec.js'",
    "beautify-check": "esbeautifier -k index.js 'specs/**/*.spec.js'",
    "eslint": "eslinter index.js 'specs/**/*.spec.js'",
    "test": "mocha-runner 'specs/**/*.spec.js'",
    "cover": "istanbul cover -x 'specs/**/*.spec.js' mocha-runner 'specs/**/*.spec.js' html text-summary",
    "watch": "npm run cover && watch-spawn -p 'specs/**/*.spec.js' -p 'index.js' -p  npm run cover",
    "lint": "npm run beautify && npm run eslint && npm test",
    "check": "npm run beautify-check && npm run eslint && npm test",
    "changelog": "changelogx -f markdown -o ./changelog.md",
    "do-changelog": "npm run changelog && git add ./changelog.md && git commit -m 'DOC: Generate changelog'",
    "install-hooks": "prepush install && changelogx install-hook",
    "bump": "bumpery --versionType=patch"
  },
  "keywords": [
    "esformatter-plugin",
    "esformatter",
    "jsx",
    "react jsx",
    "AST",
    "codestyle",
    "react jsx"
  ],
  "peerDependencies": {},
  "dependencies": {
    "extend": "^2.0.1",
    "falafel-espree": "^1.1.0",
    "js-beautify": "^1.5.6"
  },
  "devDependencies": {
    "bumpery": "^1.0.7",
    "chai": "*",
    "changelogx": "^1.0.13",
    "esbeautifier": "^2.2.3",
    "esformatter": "^0.7.0",
    "eslinter": "^2.0.1",
    "istanbul": "^0.3.15",
    "mocha-runner": "^1.0.8",
    "prepush": "^2.0.4",
    "proxyquire": "^1.5.0",
    "read-json-sync": "^1.1.0",
    "watch-spawn": "^1.0.3"
  },
  "changelogx": {
    "ignoreRegExp": [
      "BLD: Release",
      "DOC: Generate Changelog",
      "Generated Changelog"
    ],
    "issueIDRegExp": "#(\\d+)",
    "commitURL": "https://github.com/[object Object]/esformatter-jsx/commit/{0}",
    "authorURL": "https://github.com/{0}",
    "issueIDURL": "https://github.com/[object Object]/esformatter-jsx/issues/{0}",
    "projectName": "esformatter-jsx"
  },
  "gitHead": "62eda0b50878a45fe51a6d93d740529e247706e8",
  "bugs": {
    "url": "https://github.com/royriojas/esformatter-jsx/issues"
  },
  "homepage": "https://github.com/royriojas/esformatter-jsx",
